<?xml version="1.0"?>
<ruleset name="MaartenBday2023">
	<description>Mentosmenno2 MaartenBday2023 ruleset</description>
	<config name="testVersion" value="8.1-"/>
	<file>./app</file>
	<file>./index.php</file>
	<file>./autoloader.php</file>

	<arg name="colors"/>
